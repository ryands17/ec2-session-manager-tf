    "upgrade:next": "yarn add cdktf@next cdktf-cli@next"
  },
  "engines": {
    "node": ">=10.12"
  },
  "dependencies": {
    "@cdktf/provider-aws": "^1.0.9",
    "cdktf": "0.1.0",
    "constructs": "^3.3.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "cdktf-cli": "0.1.0",
    "dotenv": "^8.2.0",
    "husky": ">=4.3.8",
    "lint-staged": ">=10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,md}": "prettier --write"
  }
}
