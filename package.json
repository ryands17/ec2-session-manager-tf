{
  "name": "ec2-tf",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "yarn cdk get",
    "build": "yarn get && tsc",
    "synth": "yarn cdk synth",
    "compile": "tsc --pretty",
